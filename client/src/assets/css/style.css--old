.jumbo-btn {
  text-align: center;
  background: #19bce5;
  color: #fff;
  border: 2px solid #19bce5;
  font-size: 16px;
  font-weight: 600;
  letter-spacing: 0.06em;
  padding-top: 15px;
  padding-bottom: 15px;
  padding-left: 20px;
  padding-right: 20px;
  text-transform: uppercase;
  /* font-weight: 700; */
  -webkit-transition: 0.3s;
  -o-transition: 0.3s;
  transition: 0.3s;
  text-decoration: none;
  display: inline-block;
  margin-top: 30px;
  cursor: pointer;
  min-width: 230px;
  border-radius: 0;
