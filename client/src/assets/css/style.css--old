@import url("https://fonts.googleapis.com/css?family=Montserrat:300,400,400i,500,600,700,800");

body {
  letter-spacing: 0.5px;
  color: #555;
  font-family: "Montserrat", sans-serif;
  /*    color: #ffb8c1;*/
}

img {
  width: 100%;
}

h1,
